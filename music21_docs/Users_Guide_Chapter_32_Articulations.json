        "type": "Title"
    },
    {
        "text": "'object'",
        "type": "Title"
    },
    {
        "text": "fingerNumber",
        "type": "Title"
    },
    {
        "text": "n3",
        "type": "Title"
    },
    {
        "text": "articulations",
        "type": "Title"
    },
    {
        "text": "[]",
        "type": "UncategorizedText"
    },
    {
        "text": "n3",
        "type": "Title"
    },
    {
